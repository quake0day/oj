class Solution{
    public:
        ListNode *swapPairs(ListNode *head){
            ListNode *dummy = new ListNode(0);
            dummy->next = head;
            while(head) head = swapNodes(head->next);
        }

        ListNode *swapNodes(ListNode *&head){
                if(!head || !head->next) return NULL;
                ListNode *tail = head;
                
            }
        
};
